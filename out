CREATE TABLE [Fees] (
    [Id] int NOT NULL IDENTITY,
    [DefaultAmount] decimal(18, 2) NOT NULL,
    [Name] nvarchar(max) NULL,
    CONSTRAINT [PK_Fees] PRIMARY KEY ([Id])
);

GO

CREATE TABLE [PlanningAppFees] (
    [PlanningAppId] int NOT NULL,
    [FeeId] int NOT NULL,
    [Amount] decimal(18, 2) NOT NULL,
    CONSTRAINT [PK_PlanningAppFees] PRIMARY KEY ([PlanningAppId], [FeeId]),
    CONSTRAINT [FK_PlanningAppFees_Fees_FeeId] FOREIGN KEY ([FeeId]) REFERENCES [Fees] ([Id]) ON DELETE CASCADE,
    CONSTRAINT [FK_PlanningAppFees_PlanningApps_PlanningAppId] FOREIGN KEY ([PlanningAppId]) REFERENCES [PlanningApps] ([Id]) ON DELETE CASCADE
);

GO

CREATE INDEX [IX_PlanningAppFees_FeeId] ON [PlanningAppFees] ([FeeId]);

GO

